{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "PORT": "5000",
    "SHEET_ID": "@sheet_id",
    "GOOGLE_SERVICE_ACCOUNT_KEY": "@google_service_account_key"
  }
}
